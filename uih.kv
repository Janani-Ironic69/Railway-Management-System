MDScreen:
    screen_manager:screen_manager
    MDTopAppBar:
        pos_hint: {"top": 1}
        title:"Railway"
    MDScreenManager:
        id:screen_manager
        MDScreen:
            name:"main"
            Image:
                id:images
                source:"no.jpg"
                pos: self.pos
                size_hint:1,.894
                size: root.size
                allow_stretch:True
                keep_ratio:False

            MDRectangleFlatIconButton:
                md_bg_color:"orange"
                icon:"ticket-confirmation"
                icon_color:"black"
                text_color:"black"
                text:"TICKET BOOKING"
                size_hint_x:.322
                pos_hint:{'center_x':.1666,'center_y':.85}
                on_release:
                    root.screen_manager.current = "booking"
                    app.booking_details()

            MDRectangleFlatIconButton:
                md_bg_color:"orange"
                icon:"ticket-outline"
                icon_color:"black"
                text_color:"black"
                text:"CANCEL TICKET"
                size_hint_x:.322
                pos_hint:{'center_x':.4999,'center_y':.85}

            MDRectangleFlatIconButton:
                md_bg_color:"orange"
                icon:"eye"
                icon_color:"black"
                text_color:"black"
                text:"SHOW TICKET"
                size_hint_x:.322
                pos_hint:{'center_x':.8322,'center_y':.85}
                on_release:
                    root.screen_manager.current ="showticket"
                    app.showticket()
            MDRectangleFlatIconButton:
                md_bg_color:"orange"
                icon:"account-cash"
                icon_color:"black"
                text_color:"black"
                text:"TRANSACTION"
                size_hint_x:.322
                pos_hint:{'center_x':.3333,'center_y':.77}
            MDRectangleFlatIconButton:
                md_bg_color:"orange"
                icon:"clipboard-text-clock"
                icon_color:"black"
                text_color:"black"
                text:"TICKET HISTORY"
                size_hint_x:.322
                pos_hint:{'center_x':.6666,'center_y':.77}

            MDCard:
                orientation: "vertical"
                padding: 20
                spacing:30
                size_hint: .3,.4
                pos_hint: {"center_x": .5, "center_y": .45}
                elevation: 6
                shadow_offset: 0, -2

                MDRectangleFlatIconButton:
                    id:From
                    pos_hint:{"center_x":.5,"center_y":2}
                    size_hint_x:.75
                    text:"FROM"
                    icon:"train"
                    on_release:
                        root.screen_manager.current = "chutiya"
                        app.from_pressed()



                MDRectangleFlatIconButton:
                    id:To
                    pos_hint:{"center_x":.5,"center_y":2}
                    size_hint_x:.75
                    text:"TO"
                    icon:"train"
                    on_release:
                        root.screen_manager.current="MF"
                        app.to_pressed()
                MDRectangleFlatIconButton:
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    icon:"calendar-edit-outline"
                    text:"Date"
                    on_release: app.show_date_picker()


                MDRectangleFlatButton:
                    text:"search train"
                    pos_hint:{"center_x":.5,"center_y":2}
                    size_hint_x:.75
                    adaptive_width:True
                    on_release:
                        root.screen_manager.current = "gaandu"
                        app.search_train()

        MDScreen:
            name:"chutiya"
            MDBoxLayout:
